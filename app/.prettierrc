{
    "useTabs": true,
    "singleQuote": false,
    "trailingComma": "none",
    "printWidth": 120,
    "semi": true,
    "endOfLine": "lf",
    "tabWidth": 4,
    "plugins": [
        "prettier-plugin-svelte",
        "prettier-plugin-tailwindcss"
    ],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ],
    "tailwindStylesheet": "./src/app.css"
}